package com.boventech.demo.entity;

/**
 * 对试卷进行设置对应的实体类
 * 
 * @author Administrator
 * 
 */
public class Exam {
    // 设置单选题的数量
    private int singleChoices;

    // 设置多选题的数量
    private int multipleChoices;

    // 设置是非题的数量
    private int rightOrWrongs;

    // 设置单选题的分数
    private int singleScore;

    // 设置多选题的分数
    private int multipleScore;

    // 设置是非题的分数
    private int rightOrWrongScore;

    /**
     * @return the singleChoices
     */
    public int getSingleChoices() {
        return singleChoices;
    }

    /**
     * @param singleChoices
     *            the singleChoices to set
     */
    public void setSingleChoices(int singleChoices) {
        this.singleChoices = singleChoices;
    }

    /**
     * @return the multipleChoices
     */
    public int getMultipleChoices() {
        return multipleChoices;
    }

    /**
     * @param multipleChoices
     *            the multipleChoices to set
     */
    public void setMultipleChoices(int multipleChoices) {
        this.multipleChoices = multipleChoices;
    }

    /**
     * @return the rightOrWrongs
     */
    public int getRightOrWrongs() {
        return rightOrWrongs;
    }

    /**
     * @param rightOrWrongs
     *            the rightOrWrongs to set
     */
    public void setRightOrWrongs(int rightOrWrongs) {
        this.rightOrWrongs = rightOrWrongs;
    }

    /**
     * @return the singleScore
     */
    public int getSingleScore() {
        return singleScore;
    }

    /**
     * @param singleScore
     *            the singleScore to set
     */
    public void setSingleScore(int singleScore) {
        this.singleScore = singleScore;
    }

    /**
     * @return the multipleScore
     */
    public int getMultipleScore() {
        return multipleScore;
    }

    /**
     * @param multipleScore
     *            the multipleScore to set
     */
    public void setMultipleScore(int multipleScore) {
        this.multipleScore = multipleScore;
    }

    /**
     * @return the rightOrWrongScore
     */
    public int getRightOrWrongScore() {
        return rightOrWrongScore;
    }

    /**
     * @param rightOrWrongScore
     *            the rightOrWrongScore to set
     */
    public void setRightOrWrongScore(int rightOrWrongScore) {
        this.rightOrWrongScore = rightOrWrongScore;
    }

}
