package com.boventech.demo.util;

import java.util.HashMap;

/**
 * @author peng.xia
 * 
 */
public class RecordAnalysis {

    private HashMap<String, Double> countPercents;

    private HashMap<String, Double> rightPercents;

    /**
     * @return the countPercents
     */
    public HashMap<String, Double> getCountPercents() {
        return countPercents;
    }

    /**
     * @param countPercents
     *            the countPercents to set
     */
    public void setCountPercents(HashMap<String, Double> countPercents) {
        this.countPercents = countPercents;
    }

    /**
     * @return the rightPercents
     */
    public HashMap<String, Double> getRightPercents() {
        return rightPercents;
    }

    /**
     * @param rightPercents
     *            the rightPercents to set
     */
    public void setRightPercents(HashMap<String, Double> rightPercents) {
        this.rightPercents = rightPercents;
    }

}
